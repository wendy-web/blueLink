}


.navBar{
  width: 100%;
  height: 375rpx;
  position: fixed;
  bottom: 0;
  left: 0
}
.section{
  height: 100rpx;
  margin-top: 20rpx;
}
checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
  border: 4rpx solid #999;
}
radio .wx-radio-input{
  width: 30rpx;
  height: 30rpx;
  border: 4rpx solid #999;
  border-radius: 0;
}

radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: red;
}
/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */

.xuanze{
  padding:20rpx 0;
  color:#000;
  display:flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.jiage{
  position: relative;
}
.jiage::after{
  position: absolute;
  content: "";
  width:1rpx;
  height: 100%;
  border-right: 2rpx solid #eee;
  top: 0;
  left: 230rpx;
}


