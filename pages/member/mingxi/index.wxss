.hs_container{
  width: 100%;
  font-size:26rpx;
  background: #fff;
  height: 100%;
}
.hs_item{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  padding: 10px 20px;
  box-sizing: border-box;
  background: #fff;
  border-bottom: 1px solid #eee;
  line-height: 50rpx;
}
.hs_richang{
  background: #FE0000;
  color: #fff;
  padding: 5rpx;
  box-sizing: border-box;
  margin-right: 10rpx;
}
.hs_num{
  color:#24B2F4; 
}
.hs_nolist{
  text-align: center;
  line-height: 40rpx;
  height: 40rpx;
}
